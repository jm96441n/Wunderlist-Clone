<h1>New Todo for this List</h1>

<%= form_for @todo do |f| %>
	
	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :description %>
	<%= f.text_area :description %>
	
	<%= f.hidden_field :todo_list_id, value: session[:todo_list] %>
	
	<%= f.hidden_field :user_id, value: current_user.id %>
	
	<%= f.submit %>

<% end %>